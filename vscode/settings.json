{
  "terminal.integrated.copyOnSelection": true,
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.fontFamily": "mononoki Nerd Font",
  "terminal.integrated.shell.osx": "/usr/local/bin/fish",
  "python.formatting.provider": "yapf",
  "python.formatting.yapfArgs": [
    "--style",
    "{based_on_style: pep8, column_width: 100, blank_line_before_nested_class_or_def: true, space_between_ending_comma_and_closing_bracket: false, indent_width: 20}"
  ],
  "editor.minimap.enabled": false,
  "workbench.editor.enablePreview": false,
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "workbench.editor.splitSizing": "split",
  "editor.formatOnSaveTimeout": 2000,
  "rust-client.channel": "nightly",
  "rust-client.enableMultiProjectSetup": null,
  "editor.largeFileOptimizations": false,
  "editor.detectIndentation": false,
  "[yaml]": {},
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[rust]": {
    "editor.defaultFormatter": "matklad.rust-analyzer"
  },
  "rust-analyzer.cargo.allFeatures": true,
  "rust-analyzer.checkOnSave.allFeatures": null,
  "rust-analyzer.updates.channel": "nightly",
  "rust-analyzer.cargo.loadOutDirsFromCheck": true,
  "rust-analyzer.cargo.features": ["storage", "dashboard", "wf"],
  "rust-analyzer.diagnostics.disabled": [
    "macro-error",
    "unresolved-macro-call",
    "missing-unsafe"
  ],
  "files.associations": {
    "*.yml": "yaml"
  },
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },
  "editor.formatOnPaste": true,
  "security.workspace.trust.untrustedFiles": "open",
  "workbench.editor.untitled.hint": "hidden",
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },
  "json.maxItemsComputed": 10000,
  "redhat.telemetry.enabled": true,
  "editor.maxTokenizationLineLength": 40000,
  "rust-analyzer.diagnostics.enableExperimental": false,
  "javascript.preferences.quoteStyle": "single",
  "typescript.preferences.quoteStyle": "single",
  "prettier.singleQuote": true,
  "workbench.colorTheme": "Community Material Theme Darker High Contrast",
  "window.zoomLevel": 1,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true
}
